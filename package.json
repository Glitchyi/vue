{
  "name": "vue-profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 443",
    "build": "vue-cli-service build "
  },
  "dependencies": {
    "vue": "^3.2.13",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "typescript": "~4.5.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "vue": {
    "devServer": {
      "compress": true,
      "port": 443,
      "https": {
        "allowedHosts": [
          "glitchy.systems"
        ],
        "key": "./certs/keys.pem",
        "cert": "./certs/cert.pem"
      }
    }
  }
}
